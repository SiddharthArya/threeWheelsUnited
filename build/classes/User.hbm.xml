<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.threeWheelsUnited.User" table = "User">
      
      <property name = "aadhar" type = "String" column = "Aadhar" />
      <property name = "name" column = "Name" type = "string"/>
      <property name = "email" column = "Email" type = "string"/>
      <property name = "dob" column = "Dob" type = "string"/>
      <property name = "homeArea" column = "HomeArea" type = "string"/>
      <property name = "walletAmount" column = "WalletAmount" type = "int"/>
      
   </class>
</hibernate-mapping>